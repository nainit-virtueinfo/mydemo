{
    "executionRoleArn": "arn:aws:iam::881490117752:role/jk-role",
    "containerDefinitions": [
        {
            "name": "mydemo",
            "image": "nainitkalariya/mydemo",
            "essential": true,
            "portMappings": [
                {
                    "hostPort": 80,
                    "protocol": "tcp",
                    "containerPort": 80
                }
            ]
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "networkMode": "awsvpc",
    "cpu": "256",
    "memory": "512",
    "family": "ecs-demo"
}
